# Codex login (ChatGPT OAuth via Codex CLI)
# Optional: app-side timeout while waiting for `codex login --device-auth`
# AUIW_CODEX_LOGIN_TIMEOUT_SEC=300

# Optional DB path override
# AUIW_DB_PATH=C:\\Users\\<you>\\AppData\\Roaming\\AIUsageWatcher\\usage.db

# Auto refresh interval (seconds) - default 3600 (1 hour)
# AUIW_REFRESH_INTERVAL_SEC=3600

# Budget alert rule (token based)
# AUIW_DAILY_TOKEN_BUDGET=20000
# AUIW_WEEKLY_TOKEN_BUDGET=100000
# AUIW_ALERT_THRESHOLD_PCT=80
